{
  "expo": {
    "name": "Corona Tracker",
    "slug": "corona-tracker",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thebrandonlucas.covid19",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.thebrandonlucas.covid19",
      "versionCode": 1
    },
    "description": "Using CSSE Johns Hopkins Data at: https://github.com/CSSEGISandData/COVID-19"
  }
}
